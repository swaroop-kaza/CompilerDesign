decl
  int a,factorial(int n);
	int sum(int b,int c);
enddecl

int factorial(int n)
{
	decl
		int f;
	enddecl
	begin
		if(n==0)then
			f=1;
		else if(n==1) then
			f=1;
		else
			f=n*factorial(n-1);
		endif;
		endif;
		return f;
	end;
}

int sum(int b,int c)
{
	decl
		int q;
	enddecl
	begin 
		q=b+c;
		return q;
	end;
}

int main()
{
		decl
			int b,c,d;
		enddecl
		begin	
			read(b);
			read(c);
			a=factorial(sum(b,c));
			write(a);
		  return 0;
    end;
}
